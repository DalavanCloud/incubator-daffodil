								<xs:element name="state" type="xs:string"
									dfdl:lengthKind="explicit" dfdl:length="{ 2 }" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>

	</tdml:defineSchema>

