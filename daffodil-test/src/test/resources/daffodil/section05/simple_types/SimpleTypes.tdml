    <xs:element name="uL_01" type="xs:unsignedLong" />
    <xs:element name="d_01" type="xs:double" />
    <xs:element name="dec_01" type="xs:decimal" />
    <xs:element name="hb_01" type="xs:hexBinary" />
  </tdml:defineSchema>

