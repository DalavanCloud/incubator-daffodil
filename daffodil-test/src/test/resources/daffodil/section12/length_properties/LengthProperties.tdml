    <tdml:infoset>
      <tdml:dfdlInfoset>
        <LP_03>*+,</LP_03>
      </tdml:dfdlInfoset>
    </tdml:infoset>
  </tdml:parserTestCase>

  <!--
    Test name: LengthProp_charVsBytes2
