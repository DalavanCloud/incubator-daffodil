	<tdml:defineSchema name="lengthKindDelimited-Embedded.dfdl.xsd">
		
		

			<dfdl:format ref="ex:daffodilTest1" lengthKind="delimited" lengthUnits="bytes" encoding="UTF-8" separator="" initiator="" terminator=""
