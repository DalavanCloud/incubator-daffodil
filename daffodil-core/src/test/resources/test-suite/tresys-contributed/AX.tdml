          <block>CCCCCCCC</block>
          <block>DDDDD</block>
          <block>EEE</block>
          <block>FFFFFF</block>
          <block>GGG/!GGGGGGGGGG</block>
          <block>GGGGGGGGGGGGGG/</block>
        </list>
