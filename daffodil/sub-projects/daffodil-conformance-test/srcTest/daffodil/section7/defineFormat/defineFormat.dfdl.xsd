<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE schema>
<schema xmlns="http://www.ogf.org/dfdl/dfdl-1.0/XMLSchemaSubset"
	targetNamespace="http://www.example.org/example1/" 
	xmlns:tns="http://www.example.org/example1/" 
	xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
	xmlns:xsd="http://www.ogf.org/dfdl/dfdl-1.0/XMLSchemaSubset" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<!-- Tests chaining of named properties -->
	

	<annotation>
		<appinfo source="http://www.ogf.org/dfdl/dfdl-1.0/">
		
		<dfdl:defineFormat name="def2">
				<dfdl:format ref="tns:def3" lengthKind="explicit" lengthUnits="characters" length="4" 
					representation="text" textNumberRep="standard"/>
		</dfdl:defineFormat>
			
		<dfdl:defineFormat name="def3">
				<dfdl:format representation="binary"/>
		</dfdl:defineFormat>
		
		<dfdl:format ref="tns:def1" initiator="" terminator="" 
                encoding="utf-8"/>
		</appinfo>
	</annotation>
	
	<element name="easy" type="xsd:int"/>
	
	<annotation>
		<appinfo source="http://www.ogf.org/dfdl/dfdl-1.0/">
		<dfdl:defineFormat name="def1">
				<dfdl:format ref="tns:def2"/>
		</dfdl:defineFormat>
		</appinfo>
	</annotation>
</schema>
