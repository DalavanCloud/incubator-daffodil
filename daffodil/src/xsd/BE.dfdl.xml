<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE schema>
<schema xmlns="http://www.ogf.org/dfdl/dfdl-1.0/XMLSchemaSubset"
    targetNamespace="http://www.example.org/example1/"
    xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
	xmlns:xsd="http://www.ogf.org/dfdl/dfdl-1.0/XMLSchemaSubset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ogf.org/dfdl/dfdl-1.0/XMLSchemaSubset ./DFDLSubsetOfXMLSchema_v1_036.xsd http://www.ogf.org/dfdl/dfdl-1.0/ DFDL_part3_model.xsd">

  <!-- unordered sequences -->
  <element name="seq">
    <complexType>  
      <sequence dfdl:sequenceKind="unordered">
	<element name="x" type="xsd:string" maxOccurs="4"
		 dfdl:initiator="X:"
		 dfdl:terminator="%NL;"/>
	<element name="y" type="xsd:string" maxOccurs="4"
		 dfdl:initiator="Y:"
		 dfdl:terminator="%NL;"/>
	<element name="z" type="xsd:string" minOccurs="2" maxOccurs="4"
		 dfdl:initiator="Z:"
		 dfdl:terminator="%NL;"/>
      </sequence>
    </complexType>
  </element>

  
</schema>
