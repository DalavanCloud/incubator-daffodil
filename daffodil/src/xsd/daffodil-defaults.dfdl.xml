<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE schema>
<schema xmlns="http://www.ogf.org/dfdl/dfdl-1.0/XMLSchemaSubset"
	targetNamespace="https://opensource.ncsa.illinois.edu/daffodil/deprecated/old-defaults/"
	xmlns:daffodil-deprecated="https://opensource.ncsa.illinois.edu/daffodil/deprecated/old-defaults/" 
    xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
	xmlns:xsd="http://www.ogf.org/dfdl/dfdl-1.0/XMLSchemaSubset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ogf.org/dfdl/dfdl-1.0/XMLSchemaSubset ./DFDLSubsetOfXMLSchema_v1_036.xsd http://www.ogf.org/dfdl/dfdl-1.0/ DFDL_part3_model.xsd">

	<!-- Basic variable definition and inputValueCalc -->

	<annotation>
		<appinfo source="http://www.ogf.org/dfdl/dfdl-1.0/">
		
			<dfdl:defineVariable name="x" type="xsd:int" defaultValue="0" />

			<dfdl:defineFormat name="Daffodil_Defaults_That_Were_HardCoded">
			
				<!-- These come from the code from AnnotationDefaults.scala. So they 
					may be needed as defaults to make some examples/tests work. I've changed some 
					of the names to match the DFDL spec so this DFDL schema will validate. Two 
					properties that were in the scala code defaults have nothing corresponding 
					to them in the DFDL spec now: timingBefore='true' endOfParent='false' -->
					
				<dfdl:format alignment='1' alignmentUnits='bytes'
					textStandardBase='10' binaryFloatRep='ieee' binaryNumberRep='binary'
					byteOrder='bigEndian' calendarPatternKind='implicit' encoding='UTF-8'
					escapeSchemeRef='' documentFinalTerminatorCanBeMissing='no'
					ignoreCase='no' initiatedContent='no' leadingSkip='0' lengthKind='delimited'
					occursCountKind='parsed' separatorPolicy='required'
					separatorPosition='infix' sequenceKind='ordered' textNumberRep='standard'
					textNumberCheckPolicy='strict' textStringJustification='left'
					trailingSkip='0' />
					
			</dfdl:defineFormat>
			
			<!-- 
			Default DFDL format for this schema file and any other that want to use
			these defaults can reference them like this 
			-->
			<dfdl:format ref="daffodil-deprecated:Daffodil_Defaults_That_Were_HardCoded" />
			
		</appinfo>
	</annotation>

</schema>
